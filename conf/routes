GET         /                      controllers.Default.redirect(to = "/books")
GET         /assets/*file          controllers.Assets.versioned(path="/public", file: Asset)
GET         /books                 controllers.BookController.index()
GET         /books/register        controllers.RegisterBookController.index()
POST        /books/register        controllers.RegisterBookController.register()
GET         /books/:id/update      controllers.UpdateBookController.index(id:String)

